    package="nl.hsleiden.imtpmd">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OverzichtActivity" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ModulesActivity" android:label="Vakken"></activity>
        <activity android:name=".MainActivity" android:label="Welk jaar wilt u aanpassen"></activity>
        <activity android:name=".GradesActivity" android:label=""></activity>
        <activity android:name=".SemesterActivity" android:label="Welk semester wilt u aanpassen"></activity>
        <activity android:name=".KeuzeActivity" android:label="Is het een hoofd of keuzevak?"></activity>
    </application>

</manifest>